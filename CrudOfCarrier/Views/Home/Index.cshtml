@model IEnumerable<CrudOfCarrier.Models.Entities.Carrier>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h2>Carrier List</h2>

    @foreach (var item in Model)
    {
        <p>Carrier Name: @item.Name</p>
    }
    <a href="Home/Create">Add Carrier</a>
    <br />
    <br />
    <a href="Rating/Index">View All Rated Carriers</a>
    <br />
    <br />
    @if (ViewBag.UserLogged != null)
    {
        <a href="Rating/RateCarrier">Rate Carrier</a>
    }
</div>
